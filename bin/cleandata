#! /bin/csh -f
#
# This shell script deletes unnecessary files under a design directory.
# "@(#) cleandata version 1.1 4/27/91"
#
# ############## do not change below this line ################
onintr problem 

set test = ${?DATADIR}
if $test != 1 then
    set ddirectory = DATA
else
    set ddirectory = $DATADIR
endif
endif

echo "Cleaning data directory:$ddirectory"

/bin/rm -f $ddirectory/*

exit

problem:
echo "ERROR! Suggestion:does DATADIR environment variable exist?"
exit
